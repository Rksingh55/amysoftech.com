{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./*"]
        }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/apps/todolist.tsx", "components/Tablayout.jsx", "components/front/Pageheader.jsx", "pages/auction/index.tsx", "pages/auction/auction-preview/index.tsx", "Reducer/testSlice.jsx", "components/NextBreadcrumb.tsx", "pages/dashboard/vendor/vendor-list/index.tsx", "components/dashboardbredcrumb.jsx", "components/front/producttypecard.jsx"],
    "exclude": ["node_modules"]
}
